{
	"ua": "",
    // 首页地址 用于获取 分类和首页推荐
	"homeUrl": "https://www.nunuyy1.top/",
    // 分类节点
	"cateNode": "//ul[contains(@class,'nav')]/li/a[contains(@href, '/') and not(contains(text(), '首页'))]",
    // 分类名
	"cateName": "/text()",
    // 分类id
	"cateId": "/@href",
    // 分类id二次处理正则
	"cateIdR": "/(\\S+)/",
    // 手动设置分类，如果手动设置了分类则不使用上面的分类xpath获取分类  例如 "cateManual": {"电影": "1", "电视剧": "2"},
	"cateManual": {},
    // 首页推荐视频的节点
	"homeVodNode": "//div[@lists-content']/ul/li/",
    // 首页推荐视频的名称
	"homeVodName": "/a/img/@alt",
    // 二次处理正则
    "homeVodNameR": "",
    // 首页推荐视频的id
	"homeVodId": "/a/@href",
    // 二次处理正则
	"homeVodIdR": "/(\\S+).html",
    // 首页推荐视频的图片
	"homeVodImg": "/a/img/@src",
    // 二次处理正则
    "homeVodImgR": "",
    // 首页推荐视频的简介
	"homeVodMark": "/a/div[@class='note']/span/text()",
    // 二次处理正则
    "homeVodMarkR": "",
    // 分类页地址 {cateId} 分类id {catePg} 当前页  
	"cateUrl": "https://www.nunuyy1.top/{cateId}/",
    // 同上面的homeVod字段 分类列表中的视频信息
	"cateVodNode": "//div[@lists-content']/ul/li/",
	"cateVodName": "/a/img/@alt",
	"cateVodId": "/a/@href",
	"cateVodIdR": "/(\\S+).html",
	"cateVodImg": "/a/img/@src",
	"cateVodMark": "/a/div[@class='note']/span/text()",
    // 详情页地址 用于获取详情页信息 及 播放列表和地址
	"dtUrl": "https://www.nunuyy1.top/{vid}.html",
  
    // 详情节点
	"dtNode": "//header[@class='product-header']",
    // 视频名
	"dtName": "//h1[contains(@class,'product-title')]/text()",
	"dtNameR": "",
    // 视频图片
	"dtImg": "/img[1]/@src",
	"dtImgR": "",
    "dtCate": "/div[@class='product-excerpt' and contains(text(),'类型')]/span/a/text()",
    "dtYear": "//h1[contains(@class,'product-title')]/span[1]/text()",
    "dtYearR": "\\((\\S+)\\)",
    "dtArea": "/div[@class='product-excerpt' and contains(text(),'地区')]/span/a/text()",
    "dtActor": "/div[@class='product-excerpt' and contains(text(),'主演')]/span/a/text()",
    "dtDirector": "/div[@class='product-excerpt' and contains(text(),'导演')]/span/a/text()",
    "dtDesc": "/div[@class='product-excerpt' and contains(text(),'剧情简介')]/span/text()",
    // 播放源节点
	"dtFromNode": "//div[@class='playlists' and @id='slider']",
    // 播放源名称
	"dtFromName": "/dt[@class='on']/text()",
    // 二次处理正则
	"dtFromNameR": "",
    
    // 播放列表节点
	"dtUrlNode": "//ul[contains(@class,'stui-content__playlist']",
    // 播放地址节点
	"dtUrlSubNode": "/li/a",
    // 播放地址
	"dtUrlId": "/@href",
    // 二次处理正则
	"dtUrlIdR": "/video/(\\S+).html",
    // 剧集名称
	"dtUrlName": "/text()",
    // 二次处理正则
	"dtUrlNameR": "",
    // 播放页面的地址 {playUrl} 对应上面 dtUrlId 获取到的地址
	"playUrl": "https://www.nunuyy1.top/url.php",
    // 解析webview的user-agent
	"playUa": "",
    // 搜索地址
	"searchUrl": "https://www.nunuyy1.top/vodsearch/-------------.html?wd={wd}&submit=",
    // 同上面的homeVod字段 搜索结果中的视频信息, 这里有对苹果cms suggest搜索接口的特殊支持，参考示例中的极品影视
  "scVodNode": "//div[@class='lists-content']/ul/li",
  "scVodName": "/h2/a/text()",
  "scVodId": "/h2/a/@href",
  "scVodIdR": "/(\\S+).html",
  "scVodImg": "/a/img/@src",
  "scVodMark": "/a/div[@class='note']/span/text()"
}      